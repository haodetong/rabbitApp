getImageSize(){
  var height = Dimensions.get('window').height;
  var width = Dimensions.get('window').width;
  switch (height) {
    case 480:
      return {height:PixelRatio.get()*60};
      break;
    case 568:
      console.log("inner");
      return {height:PixelRatio.get()*85};
      break;
    case 667:
      return {height:PixelRatio.get()*121};
      break;
    default:
      return {height:PixelRatio.get()*180};
  }

}



<View style={styles.container}>
    <View style = {{flex:1, flexDirection:'row',borderWidth:1, borderColor:'#ff0000', alignItems:'flex-start'}}>
